{
  "name": "youtube-history-ralph-2026",
  "version": "1.0.0",
  "description": "YouTube Watch History Manager - Electron app for managing personal YouTube watch history",
  "type": "module",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "tsc && pnpm copy-static",
    "copy-static": "mkdir -p dist/renderer && cp -r src/renderer/*.html dist/renderer/",
    "start": "pnpm build && electron .",
    "dev": "pnpm build && electron .",
    "test": "vitest run"
  },
  "keywords": [
    "youtube",
    "history",
    "electron"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.1.0",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
